--' sound librarian - TB3D removed from bun_deactivate_radar.ltx
function zvuk_bibliotekar(sound_name)
	if sound_name then
		if snd_obj and snd_obj:playing() then
			snd_obj:stop()
		end
		snd_obj = xr_sound.get_safe_sound_object([[monsters\alligator]])
		if snd_obj then
			snd_obj:play_no_feedback(db.actor, sound_object.s2d, 0, vector(), 1.0)
		else
			if TB3D_Modders.Global_Debug then TB3D_Services.packet_alert("ARHARA DIALOG: THE SOUND IS NOT FOUND: "..sound_name) end
		end
	else
		if TB3D_Modders.Global_Debug then TB3D_Services.packet_alert("ARHARA DIALOG: THE SOUND IS NOT SET") end
	end
end

