[doc_view]:identity_immunities
GroupControlSection	= spawn_group
discovery_dependency = 
$spawn 				= "devices\quest_items\decoder_documents"
class				= II_ATTCH
cform				= skeleton
radius				= 1
quest_item			= false
slot				= 1
doc_view
;further options that can be used in specific documents in this section, they assigned a default value
visual				= equipments\item_document_01.ogf
inv_weight			= 0
cost				= 0
;icon
inv_grid_width		= 2
inv_grid_height		= 1
inv_grid_x			= 6
inv_grid_y			= 19

inv_name			= item_document_viewer
inv_name_short		= item_document_viewer_s
description			= enc_item_document_descr
;position on the screen and the number of pages
pos_x				= 100
pos_y				= 150
pages				= 1
;images folder
img_path			= auto
[doc_view_end]; !!! - service section - not to remove or use


