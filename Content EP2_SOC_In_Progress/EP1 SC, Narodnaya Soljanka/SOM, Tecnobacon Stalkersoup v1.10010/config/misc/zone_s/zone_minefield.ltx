[zone_mine_field]
GroupControlSection	= spawn_group_zone
$spawn 			= "zones\mine_field"
$prefetch 		= 16
class			= Z_MBALD
hit_impulse_scale	= 0.15;0.5 ;0.1
effective_radius	= 1 ;size range as a percentage of the original, which has a zone
sound			= 

ef_anomaly_type			= 1
ef_weapon_type			= 13

;postprocess		= mosquito_bald

BirthProbability	= 0.0
hit_type			= explosion

;----------- Anomaly settings -----------------------
min_start_power		= 0.99
max_start_power		= 1.00
attenuation			= 1
period				= 1
min_artefact_count	= 0
max_artefact_count	= 0

idle_particles		= static\mine
blowout_particles	= explosions\explosion_mine

;hit_small_particles			= 
;hit_big_particles			= 
;idle_small_particles		= 
;idle_big_particles			= 
;entrance_small_particles	= 
;entrance_big_particles		= 

;idle_sound			= 
blowout_sound		= weapons\f1_explode	;during ejection (stroke) in the center of the anomaly	
hit_sound			= anomaly\anomaly_gravy_hit1								;on the character when he gets hit
entrance_sound		= weapons\mine_on	;in contact with the object of an anomaly


awaking_time		= 50
blowout_time		= 1000	
accamulate_time		= 100000 ;200

blowout_light_time		= 150
blowout_explosion_time	= 150
blowout_particles_time	= 150
blowout_sound_time  	= 150

disable_time		= -1 ; 5000		;while ignoring the inanimate object in the zone (-1 if not required)
disable_time_small	= -1		;while ignoring the small inanimate object in the zone (-1 if not required)
disable_idle_time	= -1		;timeout idle partiklov

ignore_nonalive		= false
ignore_small		= true 
ignore_artefacts	= true

blowout_light		= on
light_color		    = 0.9,0.9,0.8
light_range		    = 30.0
light_time 		    = 0.3;0.35
light_height		= 1.5			;rise to the height of the light source


idle_light			= off

visible_by_detector = on

;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;
;creation of artifacts 
artefacts		= 

;;in response to online
spawn_blowout_artefacts = off


;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;
;; wind
blowout_wind		= on

blowout_wind_time_start     = 150
blowout_wind_time_peak      = 200
blowout_wind_time_end		= 1001;999
blowout_wind_power			= 0.5			;Wind power raises (from 0 to 1), at the moment blowout_wind_time_peak 

[zone_mine_electra]:zone_mine_field
blowout_sound		= anomaly\electra_blast1
blowout_particles	= anomaly2\electra2_blast


