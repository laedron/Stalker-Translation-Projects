[zone_teleport]
GroupControlSection	= spawn_group_zone
$spawn 			= "zones\teleport_horiz"
$prefetch 		= 16
class			= Z_MBALD

ef_anomaly_type			= 1
ef_weapon_type			= 13

sound			= zone_mosquito_bald
postprocess		= postprocess_electra

artefacts			=
BirthProbability	= 0.0

;----------- Anomaly settings -----------------------
min_start_power		= 0.00
max_start_power		= 0.00
attenuation			= 1
period				= 1
min_artefact_count	= 0
max_artefact_count	= 0


idle_particles		= anomaly2\teleport_idle
blowout_particles	= anomaly2\teleport 
tearing_particles	= anomaly2\teleport_tear
torn_particles      = anomaly2\teleport_tear

hit_small_particles			= hit_fx\hit_metal_02
hit_big_particles			= anomaly2\teleport_damage
idle_small_particles		= anomaly2\teleport_damage_smoke_00
idle_big_particles			= anomaly2\teleport_damage_smoke_00
entrance_small_particles	= anomaly2\teleport_damage
entrance_big_particles		= anomaly2\teleport_damage

tele_particles_small		= anomaly2\gravity_damage_01_smoke	;anomaly2\gravity_damage_01_smoke
tele_particles_big			= anomaly2\gravity_damage_02_smoke	


idle_sound			= anomaly\teleport_idle;  gravi_idle01							;constant sound
blowout_sound		= anomaly\teleport_work_2; anomaly_gravy_blast01	;during ejection (stroke) in the center of the anomaly	
hit_sound			= anomaly\anomaly_gravy_hit1						;on the character when he gets hit
body_tearing_sound	= anomaly\gravity_entrance					;sound of tearing of the corpse
entrance_sound		= anomaly\gravi_idle01							;in contact with the object of an anomaly

hit_type		    = strike
hit_impulse_scale	= 0.0;0.5;0.1			;the ratio of the physical momentum and energy hit
effective_radius	= 1 ;size range as a percentage of the original, which has a zone

disable_time		= 5000	;while ignoring the inanimate object in the zone (-1 if not required)
disable_time_small	= 500		;while ignoring the small inanimate object in the zone (-1 if not required)
disable_idle_time	= 100;-1	;timeout idle partiklov

ignore_nonalive		= false
ignore_small		= false
ignore_artefacts	= true

blowout_light		= on
light_color		    = 1.4,1.6,1.8
light_range		    = 30.0
light_time 		    = 0.8;0.35
light_height		= 1.5			;rise to the height of the light source

idle_light			= on
idle_light_range			= 15.0
idle_light_range_delta 		= 0.25
idle_light_anim				= teleport_stancia_in_01
idle_light_height			= 1.50			;rise to the height of the light source

awaking_time		= 0;1000
blowout_time		= 1000	
accamulate_time		= 500


blowout_light_time		= 800
blowout_explosion_time	= 800

;telekinesis parameters for the zone
throw_in_impulse			= 4000	;retracting force for unit mass at a distance of one meter
throw_out_impulse				= 1500			;ejection force blast
throw_in_impulse_alive			= 600	;pulse power retracting the living
;throw_in_atten					= 2.2	; excluded duplicates section aleks1970	;3;coeff. delays (less is more smoothly pulls)
throw_in_atten				= 		8;8			;coeff. delays (less is more smoothly pulls)
blowout_radius_percent		= 		0.8		;radius (as a percentage of the total radius), direct emissions
actor_blowout_radius_percent=		0.5

tele_height				= 0.05;	3.5	;height of telekinesis 
time_to_tele			= 5000		;time derazhaniya object in the air
tele_pause				= 5000		;pause before once again raise the fallen object

visible_by_detector = on


;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;
;; wind
blowout_wind		= on

blowout_wind_time_start     = 0
blowout_wind_time_peak      = 800
blowout_wind_time_end		= 1001; 2499
blowout_wind_power			= 0.5			; Wind power ups (from 0 to 1), at the moment blowout_wind_time_peak 



;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;
;creation of artifacts during operation
spawn_blowout_artefacts = off   ;on

