;<!-- TB3D 1.0.1e -->
[zone_gravi_zone]
GroupControlSection	= spawn_group_zone
$spawn 			= "zones\gravi zone"
$prefetch 		= 16
class			= ZS_GALAN
script_binding = amk_anoms.bind

ef_anomaly_type			= 5
ef_weapon_type			= 17

sound			= zone_mosquito_bald
;postprocess    = postprocess_gravi

artefacts		= af_vyvert,0.8, af_gravi, 0.2, af_gold_fish, 0.00
BirthProbability = 0.2;0.1

;----------- Anomaly settings -----------------------
min_start_power		= 0.99
max_start_power		= 1.00
attenuation			= 1
period				= 1
min_artefact_count	= 0
max_artefact_count	= 0


idle_particles		= anomaly2\gravity_idle
blowout_particles	= anomaly2\gravity_blast_final00 ;gravity_blast_03
tearing_particles	=anomaly2\body_tear_00
torn_particles      =anomaly2\body_tear_01

hit_small_particles			= hit_fx\hit_metal_02
hit_big_particles			= anomaly2\gravity_damage_02_blood2
idle_small_particles		= anomaly2\gravity_damage_01_smoke
idle_big_particles			= anomaly2\gravity_damage_02_smoke
entrance_small_particles	= anomaly2\gravi_anomaly_shield_00
entrance_big_particles		= anomaly2\gravi_anomaly_00;

tele_particles_small		= anomaly2\gravity_damage_01_smoke	;anomaly2\gravity_damage_01_smoke
tele_particles_big			= anomaly2\gravity_damage_02_smoke	


idle_sound			= anomaly\gravi_idle00								;constant sound
blowout_sound		= anomaly\gravi_blowout6; anomaly_gravy_blast01		;during ejection (stroke) in the center of the anomaly	
hit_sound			= anomaly\anomaly_gravy_hit1						;on the character when he gets hit
body_tearing_sound	= anomaly\anomaly_body_tear_1						;sound of tearing of the corpse
entrance_sound		= anomaly\gravi_idle01								;in contact with the object of an anomaly

hit_type		= strike
hit_impulse_scale	= 0.2;0.5;0.1		;the ratio of the physical momentum and energy hit
effective_radius	= 0.75 				;size range as a percentage of the original, which has a zone

disable_time		= 5000		;while ignoring the inanimate object in the zone (-1 if not required)
disable_time_small	= 500		;while ignoring the small inanimate object in the zone (-1 if not required)
disable_idle_time	= 100;-1	;timeout idle partiklov

ignore_nonalive		= false
ignore_small		= true
ignore_artefacts	= true

blowout_light		= on
light_color		    = 1.4,1.6,1.3
light_range		    = 30.0
light_time 		    = 0.8;0.35
light_height		= 1.5			;rise to the height of the light source

idle_light			= off

awaking_time		= 300;1000
blowout_time		= 2500	
accamulate_time		= 1000


blowout_light_time		= 2100
blowout_explosion_time	= 2100

; Options telekinesis zone
throw_in_impulse      =     1000;3000;4000   	;power pulse retraction (for a body 100kg)
throw_out_impulse			=		4000;		;ejection force blast
throw_in_impulse_alive		= 		400;600 800	;pulse power retracting the living
throw_in_atten				= 		2;8			;coeff. delay (less is more smoothly pulls)
blowout_radius_percent    	=     	0.5   		;radius (as a percentage of the total radius), direct emissions
actor_blowout_radius_percent=		0.5

tele_height				= 0.05;	3.5	;height of telekinesis 
time_to_tele			= 5000		;time derazhaniya object in the air
tele_pause				= 5000		;pause before once again raise the fallen object

visible_by_detector = on


;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;
;; wind
blowout_wind		= on

blowout_wind_time_start     = 0
blowout_wind_time_peak      = 1500
blowout_wind_time_end		= 2501; 2499
blowout_wind_power			= 0.5			;Wind power raises (from 0 to 1), at the moment blowout_wind_time_peak 



;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;
;creation of artifacts during the operation
spawn_blowout_artefacts = on

artefact_spawn_probability = 0.14;0.07				;the probability that during the operation of the anomalies will be born artifact
artefact_spawn_particles = anomaly2\gravi_anomaly_shield_00	;partikly, who played in the birth place of the artifact (optional)
artefact_born_sound =  anomaly\anomaly_gravy_hit1		;sound, who played in the birth place of the artifact (optional)

throw_out_power = 20			;momentum with which the artifact will be thrown out of the zone in any direction
artefact_spawn_height = 1		;(M) Height at the center of the area where the artifact will be born


[zone_gravi_zone_weak]:zone_gravi_zone
$spawn 			= "zones\gravi_zone_weak"
hit_impulse_scale	= 0.1
effective_radius	= 0.7

BirthProbability = 0.2
