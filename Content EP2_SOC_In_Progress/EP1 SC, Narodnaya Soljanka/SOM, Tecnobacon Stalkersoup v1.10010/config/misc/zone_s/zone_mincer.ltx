throw_in_impulse_alive			= 300	;pulse power retracting the living
throw_in_atten					= 1.2   ;coeff. delay (less is more smoothly pulls)
blowout_radius_percent			= 0.6;	;radius (as a percentage of the total radius), direct emissions
actor_blowout_radius_percent	= 0.6;

;; ветер
blowout_wind_power			= 0.2	

;----------- Anomaly settings -----------------------
min_start_power		= 0.49
max_start_power		= 0.50
attenuation			= 1
period				= 1
min_artefact_count	= 0
max_artefact_count	= 1
artefact_spawn_idle	= 24  ;how often spontaneous artifacts offline, in hours
artefact_spawn_rnd	= 50  ;probability zaspavnit artifact as a percentage.

artefacts					= af_blood,0.8, af_mincer_meat, 0.2,af_soul, 0.0
BirthProbability			= 0.1

awaking_time				= 50
blowout_time				= 10000	
accamulate_time				= 4000

; Not generating artifacts weak anomaly
[zone_mincer_weak_noart]:zone_mincer_weak
$spawn 						= "zones\mincer_zone_weak_noart"
spawn_blowout_artefacts		= off
