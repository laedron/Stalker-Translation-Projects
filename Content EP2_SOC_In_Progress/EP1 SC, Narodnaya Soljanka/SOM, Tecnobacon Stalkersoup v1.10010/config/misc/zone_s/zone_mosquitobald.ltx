[zone_mosquito_bald]
GroupControlSection	= spawn_group_zone
$spawn 			= "zones\mosquito bald"
$prefetch 		= 16
class			= ZS_MBALD
script_binding = amk_anoms.bind


hit_impulse_scale	= 0.3;0.5 ;0.1
effective_radius	= 0.85 ;size range as a percentage of the original, which has a zone
actor_blowout_radius_percent	= 0.75

ef_anomaly_type			= 1
ef_weapon_type			= 13

sound			= zone_mosquito_bald

;postprocess		= mosquito_bald


BirthProbability	= 0.1
hit_type			= strike

;----------- Anomaly settings -----------------------
min_start_power		= 0.99
max_start_power		= 1.00
attenuation			= 1
period				= 1
min_artefact_count	= 0
max_artefact_count	= 0

idle_particles		= amk_anoms\bold_idle
blowout_particles		= amk_anoms\bold_blast

hit_small_particles			= hit_fx\hit_metal_02
hit_big_particles			= anomaly2\gravity_damage_02_blood2
idle_small_particles		= anomaly2\gravity_damage_01_smoke
idle_big_particles			= anomaly2\gravity_damage_02_smoke
entrance_small_particles	= anomaly2\gravity_entrance_small
entrance_big_particles		= anomaly2\gravity_entrance_big ;anomaly2\gravity_entrance_big

idle_sound			= anomaly\gravi_rumble1; ambient\fx2		;constant sound
blowout_sound		= anomaly\anomaly_gravy_blast1; anomaly_gravy_blast01	;during ejection (stroke) in the center of the anomaly	
hit_sound			= anomaly\anomaly_gravy_hit1								;on the character when he gets hit
entrance_sound		= anomaly\anomaly_gravy_hit1								;in contact with the object of an anomaly


awaking_time		= 50;100
blowout_time		= 500	
accamulate_time		= 1000 ;10;200

blowout_light_time		= 200
blowout_explosion_time	= 200
blowout_particles_time	= 200
blowout_sound_time  	= 100

disable_time		= 5000		;while ignoring the inanimate object in the zone (-1 if not required)
disable_time_small	= 5000		;while ignoring the small inanimate object in the zone (-1 if not required)
disable_idle_time	= 100		;timeout idle partiklov

ignore_nonalive		= false
ignore_small		= true
ignore_artefacts	= true

blowout_light		= on
light_color		    = 0.5,0.4,0.3
light_range		    = 30.0
light_time 		    = 0.9;0.35
light_height		= 1.5			;rise to the height of the light source


idle_light			= off

visible_by_detector = on

;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;
;creation of artifacts 

artefacts		= af_medusa,0.7, af_cristall_flower, 0.25,af_night_star, 0.05

;;во время срабатывания в online
spawn_blowout_artefacts = on

artefact_spawn_probability = 0.09				;the probability that during the operation of the anomalies will be born artifact
artefact_spawn_particles = anomaly2\pux_blast	;partikly, who played in the birth place of the artifact (optional)
artefact_born_sound =  anomaly\pux_blast		;sound, who played in the birth place of the artifact (optional)

throw_out_power = 50			;momentum with which the artifact will be thrown out of the zone in any direction
artefact_spawn_height = 3		;(M) Height at the center of the area where the artifact will be born

;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;
;; wind
blowout_wind		= on

blowout_wind_time_start     = 0
blowout_wind_time_peak      = 300
blowout_wind_time_end		= 501; 899
blowout_wind_power			= 0.5			;Wind power raises (from 0 to 1), at the moment blowout_wind_time_peak 



