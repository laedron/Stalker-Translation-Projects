[main]
; technical parameters:
;enabled = false	;;;;;;TB3D now controlled in modders script
update_period = 2000 ;ms time between update
combat_period = 400  ; minimum time between the update in action
reload_period = 2500 ; minimum time between updates when recharging
idle_period = 10000  ; minimum time between the update in the quiescent state
return_period = 400000 ; the maximum time between returning to the prevention
sees_time = 4000       ; how to remember the enemy, for mode2 = 2
forbidden_weapons = none ; forbidden to use weapons
min_condition = 0.2    ; minimum condition in which you can use
weapon_minmaxr = false ; take parameters and min_radius max_radius of weapons config
;
[types] ; types
list = 1,5,6,7,8,9 ; list of types - only integers
line = ef_weapon_type ; parameter in the config trunk, which will be considered by its type

; for dynamic selection mode

; min_radius,max_radius ; type if weapon_minmaxr = false
minmaxr_5 = 0,50
minmaxr_6 = 1,300
minmaxr_7 = 0,35
minmaxr_8 = 25,1025
minmaxr_9 = 10,900

; modifikotory distance
rsec_5 = 50,45,30,30,30,25,20,20,20,15,10,5
rsec_6 = 2,20,30,30,30,30,25,25,25,25,20,4
rsec_7 = 2,50,60,65,60,55,45,40,30,15,5,-20
rsec_8 = -100,-40,20,30,45,55,60,60,70,80,90,100,120,140,160,180,200,220,250,200
rsec_9 = 0,10,30,50,50,50,50,50,50,30,20,0

;modifiers of the enemy
; actor,stalker,boar,bloodsucker,flesh,dog,pseudodog,cat,chimera,giant,zombie,burer,controller,poltergeist,snork,fracture,tushkano,psy_dog,psy_dog_phantom

enemy_5 = dog:20,tushkano:5,bloodsucker:-10,giant:-20
enemy_6 = fracture:10,psy_dog_phantom:20
enemy_7 = flesh:20,dog:30,pseudodog:25,zombie:20,bloodsucker:30,snork:40
enemy_8 = controller:5,dog:-30,pseudodog:-15,snork:-50,tushkano:-80,chimera:-30
enemy_9 = tushkano:20,giant:10,zombie:5,burer:5,actor:10


[default]
;+++++++++++

; prioritization of weapons:
; 0 - disabled, 1 - at 2 - to the parameters, 3 - set manually
mode1 = 2			;;;includes rain factor in calcs

;+++++++++++

; mode choice of weapons:
; in action
; 0 - engine (), 1 - highest priority, 2 - a long time to charge, 3 - Dynamic
mode2	= 3
; at rest
; 0 - engine (), 1 - highest priority, 2 - the best most of the priority type
mode2_idle	= 2 ;1

;++++++++++++

; priority types, respectively [types]- list
types = 0,1,5,7,15,20

;++++++++++++

; coefficients calculated parameters of weapons for the priority
; used to mode1 = 2
koeffs = koefs_def ; section in this file

;++++++++++++

; priority weapons, more important - higher priority
; used to mode1 = 3
priors = priors_def ; section in this file

;++++++++++++

; increasing priority in the presence of body kit,%
scope = 5 ; riflescope
silencer = 5 ; muffler
grenadelauncher = 15 ; launcher

;++++++++++++

addcost = 4000 ; value is added to the price of arms is used to mode1 = 1
process_mode = 2 ; 0 - disabled, 1 - Only during the fight, 2 - always, 3 - complete
mag_lowering = 20 ; the percentage decrease priority for empty shop
mag_lowering_fc = 35 ; the percentage drop priority when an empty shop (direct contact with the enemy)
active_add = 10 ; additive for the weapons that are already in the hands of
tchange_time = 4000 ; while the brake change weapons, ms
tchange_time_fc = 5500 ; while the brake change weapons (direct contact with the enemy)
change_rest_time = 200 ; minimum time between changing weapons, ms 1099353 2/10 of a sec (normal reaction time plus action start)

disabled = false

; sections, groups, ranks

[stalker_veteran]
mag_lowering_fc = 45

[zombied]
mode2	= 1

[dolg]
mode1	= 2

[novice]
mode1	= 2

[master]
scope = 15

